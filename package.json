{
  "name": "who-is-hiring-dashboard",
  "description": "Dashboard for Ask HN: Who is hiring?",
  "version": "0.0.3",
  "license": "MIT",
  "author": "Robert Hall <langbert@gmail.com> (http://arxpoetica.com/)",
  "homepage": "https://github.com/arxpoetica/who-is-hiring-dashboard#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arxpoetica/who-is-hiring-dashboard.git"
  },
  "volta": {
    "node": "14.5.0"
  },
  "scripts": {
    "dev": "sapper dev -p 3001",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "keyring": "gcloud kms encrypt --plaintext-file=.env.enc-source --ciphertext-file=.env.enc --location=global --keyring=whoishiring --key=cloudbuild-env",
    "publish": "gcloud builds submit --config=cloudbuild.yaml .",
    "deploy": "beta run deploy whoishiring --image gcr.io/arxpoetica/whoishiring --region us-central1",
    "bake": "run-s build publish deploy",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "watch": "trim watch --postcss --svgo"
  },
  "dependencies": {
    "ansi-colors": "4.1.1",
    "compression": "^1.7.1",
    "cross-fetch": "3.0.5",
    "dayjs": "1.8.32",
    "dexie": "3.0.2",
    "elasticlunr": "0.9.5",
    "express": "4.17.1",
    "helmet": "4.0.0",
    "morgan": "1.10.0",
    "sirv": "1.0.5"
  },
  "devDependencies": {
    "@babel/core": "7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "7.11.0",
    "@babel/runtime": "7.11.0",
    "@rollup/plugin-babel": "5.1.0",
    "@rollup/plugin-commonjs": "14.0.0",
    "@rollup/plugin-node-resolve": "8.4.0",
    "@rollup/plugin-replace": "2.3.3",
    "@sapper-dragon/postcss": "0.1.4",
    "@sapper-dragon/svgo": "0.1.4",
    "@sapper-dragon/trimmings": "0.1.4",
    "dotenv-expand": "5.1.0",
    "dotenv-extended": "2.8.0",
    "dotenv-parse-variables": "0.3.1",
    "npm-run-all": "^4.1.5",
    "rollup": "2.23.0",
    "rollup-plugin-svelte": "5.2.3",
    "rollup-plugin-terser": "6.1.0",
    "sapper": "0.27.16",
    "svelte": "3.24.0"
  }
}
